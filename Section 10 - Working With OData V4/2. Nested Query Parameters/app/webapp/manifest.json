{
  "_version": "1.59.0",
  "sap.app": {
    "id": "odata.v4.demo.trippin",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:basic",
      "version": "1.14.4",
      "toolsId": "9abf1ef3-0b24-4f40-a45b-255cc253041a"
    },
		"dataSources": {
			"mainService": {
				"uri": "/odata/v4/trippin/",
				"type": "OData",
				"settings": {
					"odataVersion": "4.0",
					"localUri": "localService/metadata.xml"
				}
			}
		}
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": false,
    "handleValidation": true,
    "dependencies": {
      "minUI5Version": "1.127.0",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {},
        "sap.f": {},
        "sap.suite.ui.generic.template": {},
        "sap.ui.comp": {},
        "sap.ui.generic.app": {},
        "sap.ui.table": {},
        "sap.ushell": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "odata.v4.demo.trippin.i18n.i18n"
        }
      },
			"": {
				"dataSource": "mainService",
				"preload": true,
				"settings": {
          "operationMode": "Server",
          "autoExpandSelect": true,
          "earlyRequests": true,
          "groupId": "$auto",
          "httpHeaders": {
            "X-My-Custom-Header": "myCustomValue"
          },
          "groupProperties": {
            "fastGroup": { "submit": "Auto" },
            "slowGroup": { "submit": "Auto" }
          }
        }
			}
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "viewType": "XML",
        "async": true,
        "viewPath": "odata.v4.demo.trippin.view",
        "controlAggregation": "pages",
        "controlId": "app",
        "clearControlAggregation": false
      },
      "routes": [
        {
          "name": "home",
          "pattern": ":?query:",
          "target": [
            "TargetHome"
          ]
        },
        {
          "name": "details",
          "pattern": "details/{userID}",
          "target": [
            "TargetDetails"
          ]
        },
        {
          "name": "edit",
          "pattern": "edit/{userID}",
          "target": [
            "TargetEdit"
          ]
        },
        {
          "name": "create",
          "pattern": "create",
          "target": [
            "TargetCreate"
          ]
        }
      ],
      "targets": {
        "TargetHome": {
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": false,
          "viewId": "Home",
          "viewName": "Home",
          "viewLevel": 1
        },
        "TargetDetails": {
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": false,
          "viewId": "Details",
          "viewName": "Details",
          "viewLevel": 2
        },
        "TargetEdit": {
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": false,
          "viewId": "Edit",
          "viewName": "Edit",
          "viewLevel": 3
        },
        "TargetCreate": {
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": false,
          "viewId": "Create",
          "viewName": "Create",
          "viewLevel": 2
        }
      }
    },
    "rootView": {
      "viewName": "odata.v4.demo.trippin.view.App",
      "type": "XML",
      "async": true,
      "id": "App"
    }
  }
}
